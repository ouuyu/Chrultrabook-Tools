{
  "build": {
    "beforeDevCommand": "npm run start",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist/chrultrabook-tools/browser",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "ChromeBook Tools",
  "version": "3.0.0",
  "identifier": "com.chrultrabook.org",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "ChromeBook Tools",
        "width": 390,
        "height": 480,
        "maximizable": false,
        "resizable": false,
        "theme": "Dark"
      }
    ]
  }
}
